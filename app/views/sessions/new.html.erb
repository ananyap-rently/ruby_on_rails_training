<h1>Login</h1>

<%= form_with url: login_path, method: :post, local: true do |form| %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </p>
  <p>
    <%= form.submit "Log in" %>
  </p>
<% end %>
